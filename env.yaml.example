# Fichier de configuration des variables d'environnement pour GCP Cloud Run
# Copier ce fichier vers env.yaml et remplir avec vos vraies valeurs

TELEGRAM_BOT_TOKEN: your_telegram_bot_token_here
DATABASE_URL: postgresql://user:password@host:port/database
MASS_NOTIFY_SECRET: your_secret_key_here
WEBHOOK_URL: https://your-cloud-run-url.run.app/webhook
ADMIN_CHAT_ID: "your_telegram_chat_id"
NODE_ENV: production
